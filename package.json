{
  "name": "ubeefurniture-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rimraf .next && rimraf node_modules/.cache && next dev",
    "postinstall": "prisma generate",
    "build": "next build",
